<div id="<%= dom_id contact %>">
  <p>
    <strong>User:</strong>
    <%= contact.user_id %>
  </p>

  <p>
    <strong>Type:</strong>
    <%= contact.type %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= contact.name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= contact.email %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= contact.phone %>
  </p>

  <p>
    <strong>Faculty:</strong>
    <%= contact.faculty %>
  </p>

  <p>
    <strong>Student id number:</strong>
    <%= contact.student_id_number %>
  </p>

  <p>
    <strong>National lecturer identification number:</strong>
    <%= contact.national_lecturer_identification_number %>
  </p>

  <p>
    <strong>Last position:</strong>
    <%= contact.last_position %>
  </p>

</div>
